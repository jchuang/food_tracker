<p><%= notice %></p>

<h1>Items in Inventory</h1>

<table>
  <thead>
    <th>Title</th>
    <th>Description</th>
    <th>Quantity</th>
  </thead>

  <tbody>
  <% @items.each do |item| %>
    <tr>
      <td><%= item.title %></td>
      <td><%= item.description %></td>
      <td><%= item.quantity %></td>
    </tr>
  <% end %>
  </tbody>
</table>
